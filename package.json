{
  "name": "companies-service",
  "version": "0.4.0",
  "description": "A service that provides information about various companies",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm run clean:dist && tsc && npm run copy:openapidoc",
    "check:prettier": "prettier --check 'src/routes.ts'",
    "clean:dist": "rm -rf dist",
    "copy:openapidoc": "cp src/middleware/openapi.yml dist/middleware/",
    "coverage": "nyc npm run test",
    "dev": "nodemon src/.bin/www",
    "lint": "eslint src",
    "start": "node dist/.bin/www.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/**/*.ts --exit",
    "test:integration": "mocha test/integration/**/*.ts --exit",
    "test:only": "mocha -- --grep \"$npm_config_file\" --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidc6/companies-service.git"
  },
  "author": "David Chubabriya (https://github.com/davidc6)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidc6/companies-service/issues"
  },
  "homepage": "https://github.com/davidc6/companies-service#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-openapi-validator": "^4.10.6",
    "express-rate-limit": "^5.2.5",
    "express-validator": "^6.9.2",
    "helmet": "^4.4.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/cors": "^2.8.9",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^8.2.0",
    "@types/node-fetch": "^2.5.8",
    "@types/pg": "^7.14.8",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.10",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
